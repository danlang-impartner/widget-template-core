    "ts-declaration-webpack-plugin": "1.2.0",
    "jasmine": "3.3.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "0.3.7",
    "prettier": "1.16.4",
    "puppeteer": "1.12.2",
    "reflect-metadata": "0.1.13",
    "ts-loader": "5.3.3",
    "ts-node": "8.0.2",
